<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.ibm.tap.sigbank.signature.SoftwareSignatureHistoryReport"
		table="SOFTWARE_SIGNATURE" lazy="true">

		<id name="softwareSignatureId" type="long"
			column="SOFTWARE_SIGNATURE_ID">
			<generator class="increment" />
		</id>

		<property name="product" column="PRODUCT" length="32"
			not-null="false" />




	</class>
</hibernate-mapping>

