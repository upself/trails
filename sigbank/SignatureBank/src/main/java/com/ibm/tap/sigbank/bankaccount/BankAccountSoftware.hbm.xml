<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ibm.tap.sigbank.bankaccount.BankAccountSoftware"
		table="BANK_ACCOUNT_SOFTWARE" lazy="true">

		<id name="bankAccountSoftwareId" type="long"
			column="BANK_ACCOUNT_SOFTWARE_ID">
			<generator class="increment" />
		</id>

		<property name="softwareOwner" column="SOFTWARE_OWNER"
			length="64" not-null="false" />

		<many-to-one name="software" column="SOFTWARE_ID"
			class="com.ibm.tap.sigbank.software.Software" not-null="true" />

		<many-to-one name="bankAccount" column="BANK_ACCOUNT_ID"
			class="com.ibm.tap.sigbank.bankaccount.BankAccount" not-null="true" />

		<property name="remoteUser" column="REMOTE_USER" length="32"
			not-null="true" />
		<property name="recordTime" column="RECORD_TIME"
			type="timestamp" not-null="true" />
		<property name="status" column="STATUS" length="32"
			not-null="true" />


	</class>
</hibernate-mapping>

