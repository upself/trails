<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ibm.tap.sigbank.filter.SoftwareFilterH"
		table="SOFTWARE_FILTER_H" lazy="true">

		<id name="softwareFilterHId" type="long"
			column="SOFTWARE_FILTER_H_ID">
			<generator class="increment" />
		</id>

		<many-to-one name="softwareFilter" column="SOFTWARE_FILTER_ID"
			class="com.ibm.tap.sigbank.filter.SoftwareFilter" not-null="true" />

		<many-to-one name="product" column="SOFTWARE_ID"
			class="com.ibm.asset.swkbt.domain.Product" not-null="true" />

		<property name="softwareName">
			<column name="SOFTWARE_NAME" length="128" not-null="true" />
		</property>

		<property name="softwareVersion">
			<column name="SOFTWARE_VERSION" length="64" not-null="true" />
		</property>

		<property name="mapSoftwareVersion"
			column="MAP_SOFTWARE_VERSION" length="64" />

		<property name="endOfSupport" column="END_OF_SUPPORT"
			type="timestamp" />

		<property name="osType" column="OS_TYPE" length="32" />

		<property name="changeJustification"
			column="CHANGE_JUSTIFICATION" length="255" not-null="true" />

		<property name="comments" column="COMMENTS" length="255" />

		<property name="remoteUser" column="REMOTE_USER" length="32"
			not-null="true" />

		<property name="recordTime" column="RECORD_TIME"
			type="timestamp" not-null="true" />

		<property name="status" column="STATUS" length="32"
			not-null="true" />
	</class>
</hibernate-mapping>

