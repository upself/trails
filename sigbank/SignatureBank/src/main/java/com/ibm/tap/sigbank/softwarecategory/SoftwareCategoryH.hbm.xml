<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ibm.tap.sigbank.softwarecategory.SoftwareCategoryH"
		table="SOFTWARE_CATEGORY_H" lazy="true">

		<id name="softwareCategoryHId" type="long"
			column="SOFTWARE_CATEGORY_H_ID">
			<generator class="increment" />
		</id>

		<many-to-one name="softwareCategory"
			column="SOFTWARE_CATEGORY_ID"
			class="com.ibm.tap.sigbank.softwarecategory.SoftwareCategory"
			not-null="true" />

		<property name="softwareCategoryName"
			column="SOFTWARE_CATEGORY_NAME" length="255" not-null="true" />

		<property name="changeJustification"
			column="CHANGE_JUSTIFICATION" length="255" not-null="true" />

		<property name="comments" column="COMMENTS" length="255" />

		<property name="remoteUser" column="REMOTE_USER" length="32"
			not-null="true" />

		<property name="recordTime" column="RECORD_TIME"
			type="timestamp" not-null="true" />

		<property name="status" column="STATUS" length="32"
			not-null="true" />

	</class>
</hibernate-mapping>

