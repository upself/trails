<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ibm.tap.sigbank.bankaccount.BankAccountFilterStat"
		table="BANK_ACCOUNT_FILTER_STAT" lazy="true">

		<id name="bankAccountFilterStatId" type="long"
			column="BANK_ACCOUNT_FILTER_STAT_ID">
			<generator class="increment" />
		</id>

		<property name="count" column="COUNT" not-null="false"
			type="long" />


		<many-to-one name="softwareFilter" column="SOFTWARE_FILTER_ID"
			class="com.ibm.tap.sigbank.filter.SoftwareFilter" not-null="true" />

		<many-to-one name="bankAccount" column="BANK_ACCOUNT_ID"
			class="com.ibm.tap.sigbank.bankaccount.BankAccount" not-null="true" />





	</class>
</hibernate-mapping>

