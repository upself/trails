<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ibm.ea.sigbank.KbDefinition" table="KB_DEFINITION" mutable="false">

		<id name="id" type="long" column="ID">
			<generator class="increment"/>
		</id>

		<property name="active" column="ACTIVE"  type="integer" />
		<property name="creationTime" column="CREATION_TIME" type="timestamp" />
		<property name="custom1" column="CUSTOM_1" length="254" />
		<property name="custom2" column="CUSTOM_2" length="254" />
		<property name="custom3" column="CUSTOM_3" length="254" />
		<property name="dataInput" column="DATA_INPUT"  type="integer" />
		<property name="definitionSource" column="DEFINITION_SOURCE" length="254" />
		<property name="deleted" column="DELETED"  type="integer" />
		<property name="description" column="DESCRIPTION" length="254" />
		<property name="externalId" column="EXTERNAL_ID" length="254" />
		<property name="guid" column="GUID" length="254" />
		<property name="modificationTime" column="MODIFICATION_TIME" type="timestamp" />	

	</class>
</hibernate-mapping>

