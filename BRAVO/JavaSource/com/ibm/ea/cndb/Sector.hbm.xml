<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ibm.ea.cndb.Sector" table="SECTOR" lazy="true" mutable="false">

		<id name="sectorId" type="long" column="SECTOR_ID">
