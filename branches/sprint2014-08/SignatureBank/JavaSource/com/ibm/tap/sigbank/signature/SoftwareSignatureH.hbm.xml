<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ibm.tap.sigbank.signature.SoftwareSignatureH"
		table="SOFTWARE_SIGNATURE_H" lazy="true">

		<id name="softwareSignatureHId" type="long" column="SOFTWARE_SIGNATURE_H_ID">
			<generator class="increment" />
		</id>

		<many-to-one name="product" column="SOFTWARE_ID"
			class="com.ibm.asset.swkbt.domain.Product" not-null="true" />

		<many-to-one name="softwareSignature" column="SOFTWARE_SIGNATURE_ID"
			class="com.ibm.tap.sigbank.signature.SoftwareSignature" not-null="true" />


		<property name="tcmId" column="TCM_ID" length="32" />

		<property name="fileName" column="FILE_NAME" length="128"
			not-null="true" />

		<property name="fileSize" type="integer" column="FILE_SIZE"
			not-null="true" />

		<property name="softwareVersion" column="SOFTWARE_VERSION"
			length="32" />

		<property name="signatureSource" column="SIGNATURE_SOURCE"
			length="32" />

		<property name="checksumQuick" column="CHECKSUM_QUICK"
			length="8" />

		<property name="checksumCrc32" column="CHECKSUM_CRC32"
			length="8" />

		<property name="checksumMd5" column="CHECKSUM_MD5" length="8" />

		<property name="endOfSupport" column="END_OF_SUPPORT" type="timestamp" />

		<property name="osType" column="OS_TYPE" length="32" />

		<property name="changeJustification" column="CHANGE_JUSTIFICATION"
			length="255" not-null="true" />

		<property name="comments" column="COMMENTS" length="255" />

		<property name="remoteUser" column="REMOTE_USER" length="32"
			not-null="true" />

		<property name="recordTime" column="RECORD_TIME" type="timestamp"
			not-null="true" />

		<property name="status" column="STATUS" length="32" not-null="true" />

	</class>
</hibernate-mapping>